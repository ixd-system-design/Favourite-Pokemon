generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// Generic model for storing data copied from third-party APIs
model item {
  id   String @id @map("_id") // e.g. "25" for Pikachu - derived from third-party data
  data Json // full JSON data from third-party API
}
